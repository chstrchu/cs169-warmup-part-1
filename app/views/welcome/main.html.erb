<h1>Login or Add User</h1>

<%= form_for :user, url =>  do |f| %>
    <p>
	    <%= f.label :username  %>
	    <%= f.text_field :username %>
	</p>
	
	<p>
		<%= f.label :password %>
		<%= f.text_field :password %>
	</p>
	   
	<p>
		<%= f.submit 'Login' %>
		<%= f.submit 'Add User'%>
	</p>
	
<% end %>
